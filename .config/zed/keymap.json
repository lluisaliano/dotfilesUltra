[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-r": "workspace::NewTerminal",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-w": "file_finder::Toggle",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-q": "terminal_panel::Toggle",
    },
  },
  {
    "context": "SettingsWindow",
    "bindings": {
      "ctrl-w": null,
    },
  },
  {
    "context": "RulesLibrary",
    "bindings": {
      "ctrl-w": null,
    },
  },
  {
    "bindings": {
      "ctrl-k ctrl-o": "workspace::Open",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-b": "workspace::ToggleLeftDock",
    },
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "space o": "editor::NewlineBelow",
    },
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "space O": "editor::NewlineAbove",
    },
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-w": null,
    },
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-w c": [
        "pane::CloseActiveItem",
        {
          "close_pinned": false,
        },
      ],
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-h": "editor::SwitchSourceHeader",
    },
  },
  {
    "bindings": {
      "alt-;": "vim::ToggleRegistersView",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-/": ["workspace::SendKeystrokes", "~"],
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-e": "project_symbols::Toggle",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-g": [
        "task::Spawn",
        { "task_name": "Start Lazygit", "reveal_target": "center" },
      ],
    },
  },
]
